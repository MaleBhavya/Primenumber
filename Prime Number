import java.util.*;
public class Main{
public static void main(String[] args) {
    int i,count=0;
    Scanner sc = new Scanner(System.in);
    int num = sc.nextInt();
    for(i=1;i<=num;i++)
    {
        if(num%i==0)
        {
            count++;
        }
    }
    if(count==2)
    {
        System.out.println(num+" is Prime number");
    }
    else
    {
        System.out.println(num+" is not Prime number");
    }
   
  }
}
